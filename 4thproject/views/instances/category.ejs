<section>
    <div>
        <%- include('../search') %>
    </div>
    <div>
        <a href="/admin/category/add">카테고리 추가</a>
        <form method="post" action="/admin/category/delete">
            <div>
                <div>
                    <input type="checkbox" id="allcheck" name="allcheck" value="allinstance" onclick="allclick(this)"> 전체 선택
                </div>
                <div>
                    <input type="submit" value="삭제">
                </div>
            </div>
            <div>
                <div>
                    <input type="checkbox">
                </div>
                <div>
                    카테고리
                </div>
            </div>
            <% for (let i = 0; i < instances.length; i++) { %>
                <div>
                    <div>
                        <input type="checkbox" id="instance" name="instance" value="<%= instances[i].category_id %>">
                    </div>
                    <div>
                        <a href="/admin/persoanl_info/<%= instances[i].category_id_id %>"><%= instances[i].category %></a>
                    </div>
                </div>
            <% } %>
        </form>
    </div>
</section>