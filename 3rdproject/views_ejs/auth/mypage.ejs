<%- include('../header') %>
    <div>
        <h1>My Page</h1>
    </div>
    <div class="mypage-body">
        <div class="mypage-btn">
            <a href="/auth/mypage">Profile</a>
            <a href="/auth/change">Change Password</a>
            <a href="/auth/leave">Leave</a>
        </div>
        <div class="mypage-profile">
            <table>
                <thead>
                    <tr>
                        <td class="padding-5-10-5-10" colspan="2">Profile</td>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td class="padding-5-10-5-10">ID</td>
                        <td class="padding-5-10-5-10"><%= UserData.user_id %></td>
                    </tr>
                    <tr>
                        <td class="padding-5-10-5-10">Nickname</td>
                        <td class="padding-5-10-5-10"><%= UserData.nickname %></td>
                    </tr>
                    <tr>
                        <td class="padding-5-10-5-10">Phone</td>
                        <td class="padding-5-10-5-10"><%= UserData.phone_number %></td>
                    </tr>
                    <tr>
                        <td class="padding-5-10-5-10">Email</td>
                        <td class="padding-5-10-5-10"><%= UserData.email %></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
<%- include('../footer') %>